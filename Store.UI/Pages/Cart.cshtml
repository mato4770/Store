@page
@model Store.UI.Pages.CartModel
@{
}
<div>
    @foreach (var product in Model.Cart)
    {
        <p>@product.Name</p>
        <p>@product.Price</p>
        <p>@product.Qty</p>
        <p>@product.StockId</p>
        <hr />
    }

</div>

<hr />
<vc:cart view="Small"></vc:cart>
<vc:cart view="Default"></vc:cart>


@await Component.InvokeAsync("Cart", new { view = "Small" })